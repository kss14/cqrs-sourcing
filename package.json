{"name":"node-demo","version":"1.0.0","description":"","main":"index.js","scripts":{"start":"npx nodemon server.js","build":"pnpm install && pnpm run dc:glitchtip & pnpm run dc:microservices &","command-kill-port":"echo \"npm run kill-port --port=3000\"","dc:microservices":"docker-compose -f ./MicroServices/docker-compose.yml up --build","dc:glitchtip":"docker-compose -f ./GlitchTip/docker-compose.yml up --build","preinstall":"node merge.js && prettier-package-json --write package.json","kill-port":"kill -9 $(lsof -t -i:$npm_config_port)","test":"echo \"Error: no test specified\" && exit 1","docker:image:remove_all":"docker rmi -f $(docker images -aq)","docker:container:remove_all":"docker rm -vf $(docker ps -aq)"},"keywords":[],"author":"","license":"ISC","dependencies":{"@sentry/node":"^7.26.0","express":"^4.17.1","mongoose":"^5.9.6","nodemailer":"^6.8.0","node-uuid":"^1.4.8","atob":"^2.1.2","amqplib":"0.5.5 && 0.5.5","btoa":"^1.2.1","merge-package-json":"^0.1.3"},"devDependencies":{"nodemon":"2.0.2 && 2.0.2","glob":"^8.0.3","npm-run-all":"^4.1.5"}}